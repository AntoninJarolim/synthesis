// herman's self stabilising algorithm [Her90]
// gxn/dxp 13/07/02

// the procotol is synchronous with no nondeterminism (a DTMC)
dtmc

const int MAXMEM = 1;

const double P_START=0;
const double P_STEP=0.01;

const int M0LFAIR=50;
const int M0HFAIR=50;
const int M1LFAIR=50;
const int M1HFAIR=50;
const int MxxA=1;
const int MxxB=1;
const int MxxC=0;

module initialise
    start : [0..1] init 0;
    [go] start = 0 -> (start'=1);
    [step1] start = 1 -> true;
endmodule

// full step counter
rewards "steps"
    round = 0 : 1;
endrewards

// label - stable configurations (1 token)
label "stable" = round =1 & num_tokens=1;

const int STATIONS = 11;

module controller
	round : [0..STATIONS] init 1;

	[sync] round = 0 -> (round'=1);
	[step1] round = 1 -> (round'=2);
	[step2] round = 2 -> (round'=3);
	[step3] round = 3 -> (round'=4);
	[step4] round = 4 -> (round'=5);
	[step5] round = 5 -> (round'=6);
	[step6] round = 6 -> (round'=7);
	[step7] round = 7 -> (round'=8);
	[step8] round = 8 -> (round'=9);
	[step9] round = 9 -> (round'=10);
	[step10] round = 10 -> (round'=11);
	[step11] round = STATIONS -> (round'=0);

endmodule


formula num_tokens = (x1=x2?1:0)+(x2=x3?1:0)+(x3=x4?1:0)+(x4=x5?1:0)+(x5=x6?1:0)+(x6=x7?1:0)+(x7=x8?1:0)+(x8=x9?1:0)+(x9=x10?1:0)+(x10=x11?1:0)+(x11=x1?1:0);

